{
	"name": "CSS Stats",
	"description": "Display selector count for each stylesheet.",
	"version": "1",
	"permissions": ["<all_urls>"],
	"browser_action": {
		"default_icon": "images/icon-16.png",
		"default_popup": "popup.html"
	},
	"icons": {
  		"16": "images/icon-16.png",
    	"48": "images/icon-48.png",
    	"128": "images/icon-128.png" },
  	"manifest_version": 2
}
